    "@polkadot/react-identicon": "^0.57.3",
    "@polkadot/types": "^1.28.1",
    "@polkadot/ui-keyring": "^0.57.3",
    "@polkadot/ui-settings": "^0.57.3",
    "@polkadot/util": "^2.18.1",
    "@polkadot/util-crypto": "^2.18.1",
    "@polkadot/keyring": "^3.1.1",
    "bignumber.js": "^9.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.2",
    "react-tippy": "^1.4.0",
    "react-is": "^16.13.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.0"
  },
  "devDependencies": {
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint": "^6.6.0",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "start": "PORT=8000 react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src/**",
    "lint:ci": "eslint src/** --max-warnings=0",
    "lint:fix": "eslint --fix src/**",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "standard"
    ],
    "plugins": [
      "only-warn"
    ],
    "rules": {
      "semi": [
        1,
        "always"
      ],
      "no-extra-semi": 1
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "substrate",
    "substrate-ui",
    "polkadot-js"
  ]
}
